@article{ramachandran2011,
	title = {Thermodynamic Stability of Histone H3 Is a Necessary but not Sufficient Driving Force for its Evolutionary Conservation},
	volume = {7},
	url = {http://dx.doi.org/10.1371/journal.pcbi.1001042},
	doi = {10.1371/journal.pcbi.1001042},
	abstract = {Most proteins fold to a well-defined, three-dimensional structure, which can be delineated into the protein surface and its buried core. When comparing amino acid sequences of the same protein from different organisms, we would expect to find certain residue positions conserved due to the importance of that position in either maintaining the protein's function or its three-dimensional structure. In this study, we looked at residues in the buried core domains of histone proteins H3 and H4, which have no known function other than maintaining the three-dimensional structure of the protein. We find that perturbing protein stability (which is a measure of maintenance of the protein's structure) by mutating these residues compromises survival fitness in yeast. However, the stability conferred by buried amino acids of H3 alone cannot account for their evolutionary conservation, which is in striking contrast to other proteins where stability has been shown to be the driving force for sequence conservation. This conservation of H3 thus points to either new additional functions of H3 that have not been uncovered or a unique conservation mechanism that goes beyond survival pressure. These data therefore reveal a highly conserved domain that is distinct in its evolutionary conservation.},
	number = {1},
	journal = {{PLoS} Comput Biol},
	author = {Ramachandran, Srinivas and Vogel, Lisa and Strahl, Brian D. and Dokholyan, Nikolay V.},
	month = jan,
	year = {2011},
	pages = {e1001042}
},

