	author = {Ron Ausbrooks and Stephen Buswell and David Carlisle and St\'{e}phane Dalmas and Stan Devitt and Angel Diaz and Max Froumentin and Roger Hunter and Patrick Ion and Michael Kohlhase and Robert Miner and Nico Poppelier and Bruce Smith and Neil Soiffer and Robert Sutor and Stephen Watt},
