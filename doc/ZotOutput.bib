@misc{ronausbrooks2003,
	title = {Mathematical Markup Language {(MathML)} Version 2.0 {(Second} Edition)},
	copyright = {Copyright {\textcopyright} 1998-2003 {W3C{\textregistered}} {(MIT}, {ERCIM}, Keio), All Rights Reserved},
