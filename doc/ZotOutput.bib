@article{pabinger2011,
	title = {{MEMOSys:} Bioinformatics platform for genome-scale metabolic models},
	volume = {5},
	issn = {1752-0509},
	lccn = {0002},
	shorttitle = {{MEMOSys}},
	url = {http://www.biomedcentral.com/1752-0509/5/20},
	doi = {10.1186/1752-0509-5-20},
	abstract = {{BACKGROUND:Recent} advances in genomic sequencing have enabled the use of genome sequencing in standard biological and biotechnological research projects. The challenge is how to integrate the large amount of data in order to gain novel biological insights. One way to leverage sequence data is to use genome-scale metabolic models. We have therefore designed and implemented a bioinformatics platform which supports the development of such metabolic {models.RESULTS:MEMOSys} {(MEtabolic} {MOdel} research and development System) is a versatile platform for the management, storage, and development of genome-scale metabolic models. It supports the development of new models by providing a built-in version control system which offers access to the complete developmental history. Moreover, the integrated web board, the authorization system, and the definition of user roles allow collaborations across departments and institutions. Research on existing models is facilitated by a search system, references to external databases, and a feature-rich comparison mechanism. {MEMOSys} provides customizable data exchange mechanisms using the {SBML} format to enable analysis in external tools. The web application is based on the Java {EE} framework and offers an intuitive user interface. It currently contains six annotated microbial metabolic {models.CONCLUSIONS:We} have developed a web-based system designed to provide researchers a novel application facilitating the management and development of metabolic models. The system is freely available at {http://www.icbi.at/MEMOSys.}},
	number = {1},
	journal = {{BMC} Systems Biology},
	author = {Pabinger, Stephan and Rader, Robert and Agren, Rasmus and Nielsen, Jens and Trajanoski, Zlatko},
	year = {2011},
	pages = {20}
},

@inproceedings{alexandremartins2009,
	address = {Rio de Janeiro. Brasil},
	title = {{WikiModels:} a platform for colaborative development of biochemical models},
	lccn = {0000},
	abstract = {Kinetic models of biochemical systems can encompass a wide range of biochemical topics, some of which still poorly understood. This fact has the following implications. First, model development often requires engaging multiple experimental as well as computational experts in an iterative testing and refinement process. Second, models always rely on a number of assumptions that are subject to discussion and subsequent change. Third, thorough documentation of assumptions, underlying rationale, and discussions eventually leading to improvement are critical for effective development. Fourth, abstracted from this contextual information, models are of little use to third parties and prone to misinterpretation. However, although good tools for simulation and analysis of kinetic models {[Nat.} Biotechnol. 24:667-672, 2006], standards such as {SBML} {[Bioinformatics} 19:524-531, 2003] for porting models, and a centralized repository {(BioModels)} {[Nucleic} Acids Res. {34:D689-D691}, 2006] are available, good publicly available tools for assisting and thoroughly documenting collaborative model development remain lacking.
We are addressing this concern by developing {WikiModels} - a Web-based platform for distributed collaboration in iterative development of kinetic models. The current prototype permits editing, browsing and documenting models. Each entity (reactions, parameters, etc.) of a model can root threaded comments (discussion lists) thus enabling an open peer-review of each aspect. A unique immutable {URL} is associated to each element and comment, allowing external cross-reference. Models can be exported as {SBML} files. 
The platform follows the principles of a {RESTful} architecture and heavily borrows from Semantic Web ideas and technologies. 
Ongoing developments include a Wiki-like history for models, as well as a {Role-Based} Access Control system. Future developments will include a simulation engine and tools for model analysis and assessment.},
	booktitle = {{VII} Iberoamerican Congress of Biophysics},
	author = {Alexandre Martins and Gon\c{c}alo Neto and Rog\'{e}rio de Lemos and Penousal Machado and Armindo Salvador},
	year = {2009}
},

@article{runco2004,
